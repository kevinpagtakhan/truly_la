<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for :session, :url => login_path do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username %><br>
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %><br>
  <%= f.submit "Login" %>
<% end %>
