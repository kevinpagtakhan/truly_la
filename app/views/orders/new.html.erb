<h1>Orders#new</h1>
<p>Find me in app/views/orders/new.html.erb</p>

<%= form_for :order, url: user_orders_path do |f| %>
  <%= f.label :shipping_street %>
  <%= f.text_field :shipping_street %><br>
  <%= f.label :shipping_street_2 %>
  <%= f.text_field :shipping_street_2 %><br>
  <%= f.label :shipping_city %>
  <%= f.text_field :shipping_city %><br>
  <%= f.label :shipping_state %>
  <%= f.text_field :shipping_state %><br>
  <%= f.label :shipping_zip_code %>
  <%= f.text_field :shipping_zip_code %><br>
  <%= f.submit "Order" %>
<% end %>
